#include<stdio.h>
int main()
{ int i,j,n,a[60],smallest;
    printf("enter size");
    scanf("%d",&n);
    printf("list the eles");
    for(i=0;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }
    smallest=a[0];
    for(j=0;j<=n;j++)
    {
        if(a[j]<smallest)
        smallest=a[j];
    }
    printf("smallest no is %d",smallest);
}
    

